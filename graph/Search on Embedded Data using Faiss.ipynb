    "dimension = 300\n",
    "fastIndex = faiss.IndexFlatL2(dimension)\n",
    "for sent in tqdm(data):\n",
    "    try:\n",
    "        fastIndex.add(np.array([data[sent]]).astype('float32'))\n",
    "    except Exception as e:\n",
    "        print(str(e))\n",
    "        pass"
   ]
